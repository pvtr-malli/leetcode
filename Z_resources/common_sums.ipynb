   "source": [
    "# get all subarrays.\n",
    "def get_all_subarrays(nums):\n",
    "    n = len(nums)\n",
    "    subarrays = []\n",
    "\n",
    "    for i in range(n):\n",
    "        for j in range(i, n):\n",
    "            subarrays.append(nums[i:j+1])\n",
    "\n",
    "    return subarrays\n",
    "\n",
    "nums = [1, 2, 3]\n",
    "print(get_all_subarrays(nums))\n"
   ]
